{
	"manifest_version": 2,
	"name": "Circularise Improvements",
	"version": "0.1.0",
	"description": "A chrome extension that modifies and improves the Circularise technology page.",
	"content_scripts": [
		{
			"css": ["styles.css"],
			"js": ["content.js"],
			"matches": ["https://www.circularise.com/technology"],
			"run_at": "document_start"
		}
	],
	"browser_action": {
		"default_icon": "icon/transparent.png"
	},
	"web_accessible_resources": ["assets/*.png"]
}
